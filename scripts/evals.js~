var dataVersion = 0;

//var xhrVersion = new Xhr('');


var menuItems;


function isDataChanged() {
	return true;
}

function getCalcData (callback) {
		//menudata
		Xhr.load('data/menu.json',
			{
				spinner: 'spinner',
				onSuccess: 
					function(request) {
						menuItems = request.responseJSON;
						callback();
					},
				onComplete: //dummy helper to use when work without app server
					function(request, rqt){
						if ((request.status == 0) && (rqt.readyState == 4)) {
							menuItems = JSON.decode(request.responseText);
							//menuItems = eval(request.responseText);
							callback();
						}
				}
			}
		);
		
}